{
  "name": "mithril-redux",
  "version": "1.0.0",
  "description": "Experimenting with mithril + redux",
  "scripts": {
    "build": "node build/browserify.js",
    "debug": "iron-node .",
    "start": "nodemon build/browserify.js --watch . --ignore output --ext js,css"
  },
  "author": "Pat Cavit <pat@patcavit.com>",
  "license": "MIT",
  "repository": "tivac/mithril-redux",
  "dependencies": {
    "mithril": "^0.2.0",
    "redux": "^3.0.2"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "nodemon": "^1.4.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "curly": 2,
      "dot-notation": 2,
      "eol-last": 0,
      "key-spacing": [
        2,
        {
          "beforeColon": true,
          "afterColon": true,
          "align": "colon"
        }
      ],
      "no-caller": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-ex-assign": 2,
      "no-extend-native": 2,
      "no-extra-parens": 0,
      "no-extra-semi": 2,
      "no-floating-decimal": 2,
      "no-func-assign": 2,
      "no-implied-eval": 2,
      "no-inner-declarations": 2,
      "no-mixed-requires": 0,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-negated-in-lhs": 2,
      "no-new": 0,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-trailing-spaces": 0,
      "no-underscore-dangle": 0,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  }
}
